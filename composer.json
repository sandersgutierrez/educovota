{
    "name": "sandersgutierrez/ses-app",
    "description": "Student Electoral System",
    "type": "project",
    "homepage": "http://github.com/sandersgutierrez/ses-app",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanders Guti√©rrez",
            "email": "ing.sanders@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "require": {
        "php": "^7.3",
        "slim/slim": "4.*",
        "slim/psr7": "^1.0",
        "slim/twig-view": "^3.0",
        "php-di/php-di": "^6.1",
        "doctrine/dbal": "^2.9",
        "laminas/laminas-diactoros": "^2.2",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit --colors=always --testdox tests/"
    }
}
